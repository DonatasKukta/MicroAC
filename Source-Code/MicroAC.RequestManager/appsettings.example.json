{
  "InternalAuthorizationRoute": "MicroAC.ServiceFabric/MicroAC.Authorization/Authorize",
  "InternalGateway": "",
  "HeadersToIgnore": [
    "Content-Length"
  ],
  "EndpointRoutes": [
    {
      "ExternalRoute": "/Authentication",
      "InternalRoute": "MicroAC.ServiceFabric/MicroAC.Authentication",
      "RequiresAuhtentication": false,
      "RequiresAuthorization": false
    }
  ],
  "Timestamp": {
    "Enabled":  true,
    "Header": "MicroAC-Timestamp",
    "ServiceName": "RequestManager"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
