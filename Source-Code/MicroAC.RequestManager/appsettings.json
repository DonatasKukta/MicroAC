{
  "InternalAuthorizationRoute": "MicroAC.ServiceFabric/MicroAC.Authorization/Authorize",
  "InternalGateway": "http://localhost:19083/",
  "HeadersToIgnore": [
    "Host",
    "User-Agent",
    "Cache-Control",
    "Content-Length"
  ],
  "EndpointRoutes": [
    {
      "ExternalRoute": "/Authentication",
      "InternalRoute": "MicroAC.ServiceFabric/MicroAC.Authentication",
      "RequiresAuhtentication": false,
      "RequiresAuthorization": false
    },
    {
      "ExternalRoute": "/ResourceApi",
      "InternalRoute": "MicroAC.ServiceFabric/Example.ResourceApi",
      "RequiresAuhtentication": false,
      "RequiresAuthorization": false
    }
  ],
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
